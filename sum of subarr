#include<iostream>
using namespace std;
int main(){
    int n,arr[n];
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int k=0;
    for(int i=0;i<n;i++){
        k=0;
        for(int j=i;j<n;j++){
            k+=arr[j];
           cout<<arr[j];
           cout<<endl;
        }
    }
   return 0; 
}
